INSERT INTO S_SYNC
	VALUES ("e55ee3f4-57c5-4c93-aa27-328809b20833",
	"00000000-0000-0000-0000-000000000000",
	'gain',
	'',
	'select any leader from instances of leader;
gain = 0;
if ( leader.distance < leader.prevDistance - GAIN_WINDOW )
  LOG::LogInfo( message:"Speed adjustment - slowing down" );
  gain = GAIN_SLOW_DOWN;
elif ( leader.distance > leader.prevDistance + GAIN_WINDOW)
  LOG::LogInfo( message:"Speed adjustment - speeding up" );
  gain = GAIN_SPEED_UP;
end if;
return gain;',
